<% provide(:title, "ユーザー登録") %>

<div class="form_box  col-sm-6">
  <h2>ユーザー登録</h2>
  <%= render "shared/error_messages" %>
  <%= form_with model: @user, url: admin_users_path do |form| %>
    <%= render 'shared/user_form', form: form %>
    <%= render 'admin_form', form: form %>
  <%= form.submit '作成', class: 'btn btn-primary' %>
  <% end %>
</div>
